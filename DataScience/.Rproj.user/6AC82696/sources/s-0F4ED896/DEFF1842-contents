# load data
mais <- read.table(url("https://imag.umontpellier.fr/~toulemonde/filesIG4/CoursAD/Donnees/Mais.txt"))
str(mais)
mais$Engrais <- as.factor(mais$Engrais)
mais$Varietes <- as.factor(mais$Varietes)

summary(aov(mais$prod~mais$Engrais*mais$Varietes))

